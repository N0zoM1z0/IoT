config common 'settings'
    option ports '1 2 3 4'
    option vlan_type '8021q'

config port '1'
    option service 'lan'
    option speed '2.5G'
    option link_mode '0'
    option switch 'switch1'
    option cpu_port '5'
    option phy_id '4'
    option base_iface 'eth0'
    option type 'eth'
    option label 'LAN1'
    option vid '1'
    option ifname 'eth0.1'

config port '2'
    option service 'lan'
    option speed '2.5G'
    option link_mode '0'
    option switch 'switch1'
    option cpu_port '5'
    option phy_id '3'
    option base_iface 'eth0'
    option type 'eth'
    option label 'LAN2'
    option vid '2'
    option ifname 'eth0.2'

config port '3'
    option service 'lan'
    option speed '2.5G'
    option link_mode '0'
    option switch 'switch1'
    option cpu_port '0'
    option phy_id '2'
    option base_iface 'eth1'
    option type 'eth'
    option label 'LAN3'
    option vid '3'
    option ifname 'eth1.3'

config port '4'
    option service 'lan'
    option speed '2.5G'
    option link_mode '0'
    option switch 'switch1'
    option cpu_port '0'
    option phy_id '1'
    option base_iface 'eth1'
    option type 'eth'
    option label 'LAN4'
    option vid '4'
    option ifname 'eth1.4'
