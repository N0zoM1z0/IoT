config device
        option name 'eth0'

config device
        option name 'eth1'

config interface 'loopback'
        option ifname 'lo'
        option proto 'static'
        option ipaddr '127.0.0.1'
        option netmask '255.0.0.0'

config switch 'switch0'
        option name 'switch0'

config switch 'switch1'
        option name 'switch1'
        option enable_vlan '1'
        option reset '1'

config switch_vlan 'vlan0'
        option device 'switch1'
        option vlan '0'
        option vid '0'
        option ports '0 1 2 3 4 5'

config switch_ext
        option device 'switch1'
        option name 'PortvlanSVlanTPID'
        option stag_tpid '0x8100'

config switch_ext
        option device 'switch1'
        option name 'PortvlanQinqMode'
        option qinq_mode 'stag'

config switch_ext 'svid_1'
        option device 'switch1'
        option name 'PortvlanDefaultSvid'
        option port_id '1'
        option default_stag_vid '4'

config switch_ext 'svid_2'
        option device 'switch1'
        option name 'PortvlanDefaultSvid'
        option port_id '2'
        option default_stag_vid '3'

config switch_ext 'svid_3'
        option device 'switch1'
        option name 'PortvlanDefaultSvid'
        option port_id '3'
        option default_stag_vid '2'

config switch_ext 'svid_4'
        option device 'switch1'
        option name 'PortvlanDefaultSvid'
        option port_id '4'
        option default_stag_vid '1'

config switch_ext
        option device 'switch1'
        option name 'PortvlanQinqRole'
        option port_id '0'
        option qinq_role 'core'

config switch_ext
        option device 'switch1'
        option name 'PortvlanQinqRole'
        option port_id '1'
        option qinq_role 'edge'

config switch_ext
        option device 'switch1'
        option name 'PortvlanQinqRole'
        option port_id '2'
        option qinq_role 'edge'

config switch_ext
        option device 'switch1'
        option name 'PortvlanQinqRole'
        option port_id '3'
        option qinq_role 'edge'

config switch_ext
        option device 'switch1'
        option name 'PortvlanQinqRole'
        option port_id '4'
        option qinq_role 'edge'

config switch_ext
        option device 'switch1'
        option name 'PortvlanQinqRole'
        option port_id '5'
        option qinq_role 'core'

config interface 'lan'
        option ifname 'eth0.1 eth0.2 eth1.3 eth1.4'
        option force_link '1'
        option type 'bridge'
        option proto 'static'
        option ipaddr '192.168.31.1'
        option netmask '255.255.255.0'
        option multicast_querier '0'
        option igmp_snooping '0'
        option ip6assign '64'

config alias 'lan_ipv6_alias'
        option interface 'lan'
        option proto 'static'
        option ip6addr 'fd00:6969:6969::1'

config interface
        option ifname 'eth0'
        option keepup '1'

config interface
        option ifname 'eth1'
        option keepup '1'

config interface 'wan'
        option ifname ''
        option proto 'dhcp'
        option ipv6 '1'
        option wantype 'eth'

config interface 'wan_2'
        option disabled '1'
        option ifname ''
        option proto 'dhcp'
        option ipv6 '0'
        option wantype 'eth'

config interface 'miot'
        option ifname 'wl13'
        option type 'bridge'
        option proto 'static'
        option ipaddr '192.168.32.1'
        option netmask '255.255.255.0'

config switch_vlan 'vlan1'
        option device 'switch1'
        option ports '4 5t'
        option vlan '1'
        option vid '1'

config switch_vlan 'vlan2'
        option device 'switch1'
        option ports '3 5t'
        option vlan '2'
        option vid '2'

config switch_vlan 'vlan3'
        option device 'switch1'
        option ports '2 0t'
        option vlan '3'
        option vid '3'

config switch_vlan 'vlan4'
        option device 'switch1'
        option ports '1 0t'
        option vlan '4'
        option vid '4'

config interface 'eth0_1'
        option ifname 'eth0.1'

config interface 'eth0_2'
        option ifname 'eth0.2'

config interface 'eth1_3'
        option ifname 'eth1.3'

config interface 'eth1_4'
        option ifname 'eth1.4'
