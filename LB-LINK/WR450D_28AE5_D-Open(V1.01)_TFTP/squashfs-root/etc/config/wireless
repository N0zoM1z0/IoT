
config wifi-device 'mt7628'
	option device 'mt7628'
	option type 'mt7628'
	option vendor 'ralink'
	option band '2.4G'
	option country 'CN'
	option autoch_skip '14'
	option bw '0'
	option ht_bsscoexist '1'
	option radio '1'
	option txpower '100'
	option wsc_confmode '7'
	option wsc_confstatus '2'
	option lowrssi '0'
	option led '1'
	option apcliauthmode 'WPA2PSK'
	option apcliencryptype 'AES'
	option apclissid 'LB-LINK FREE WIFI'
	option apcliwpapsk 'blink'
	option channel '0'
	option apclienable '0'

config wifi-iface
	option device 'mt7628'
	option ifname 'ra0'
	option network 'lan'
	option mode 'ap'
	option key '12345678'
	option maxassoc '0'
	option hidden '0'
	option macfilter 'disable'
	option disabled '0'
	option instance '1'
	option ssid '2G-mt7628-dlink'
	option encryption 'psk-mixed+ccmp'

