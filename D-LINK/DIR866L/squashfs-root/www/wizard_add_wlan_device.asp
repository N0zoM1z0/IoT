<script>
function show_info(wkey, wtype, band)
{
	var twkey = "#"+wkey;
	var twssid = twkey + "_ssid";
	var ssid = "", zone = "0";
	var twsec_type = $(twkey + "_security").val().split("_");
	var tband = band === "24G" ? "0" : "1";

	if (wtype === "host") {
		ssid = "#ssid_" + tband;
		wtype = "";
	} else {
		ssid = "#"+wtype+"_ssid_" + tband;
		wtype = wtype+"_";
	}
	$(ssid).text($(twssid).val());

	if (twsec_type[0] === "disable") { // None
		var sec_mode = "#" + wtype + "secu_mode_" + tband;
		$(sec_mode).text("NONE");
	} else if (twsec_type[0] === "wep") { // WEP
		var wep = "#" + wtype + "wep_" + tband;
		var wdef_key = $(twkey+"_wep_default_key").val();
		var wep_key = $(twkey+"_wep64_key_1").val();
		var sec_mode = "#" + wtype + "secu_mode_" + tband;
		$("#"+ wtype + "show_wep_key_" + tband).text("WEP KEY " + wdef_key + " : " + wep_key);
		if (twsec_type[1] === "open") {
			$(sec_mode).text("WEP - Open");
		}else if(twsec_type[1] === "share"){
			$(sec_mode).text("WEP - Shared Key");
		}
		$(wep).show();

	} else if(twsec_type[0] === "wpa" || twsec_type[0] === "wpa2" || twsec_type[0] === "wpa2auto"){	//WPA
		var sec_mode = "#" + wtype + "secu_mode_" + tband;
		var wpa = "#" + wtype + "wpa_" + tband;
		var psk = "#" + wtype + "psk_" + tband;
		var wpa_type = "#" + wtype + "wpa_psk_type_" + tband;
		var wpa_psk_key = "#" + wtype + "wpa_psk_key_" + tband;

		if(twsec_type[0] === "wpa" && twsec_type[1] == "psk"){		
			$(sec_mode).text("WPA - PSK");
			$(psk).show();
		}else if(twsec_type[0] === "wpa" && twsec_type[1] === "eap"){
			$(sec_mode).text("WPA - EAP");
		}else if(twsec_type[0] === "wpa2" && twsec_type[1] === "psk"){
			$(sec_mode).text("WPA2 - PSK");
			$(psk).show();
		}else if(twsec_type[0] === "wpa2" && twsec_type[1] === "eap"){
			$(sec_mode).text("WPA2 - EAP");
		}else if(twsec_type[0] === "wpa2auto" && twsec_type[1] === "psk"){
			$(sec_mode).text("Auto (WPA or WPA2) - PSK");
			$(psk).show();
		}else if(twsec_type[0] === "wpa2auto" && twsec_type[1] === "eap"){
			$(sec_mode).text("Auto (WPA or WPA2) - EAP");
		}
		$(wpa).show();
		$(wpa_type).text($(twkey + "_psk_cipher_type").val());
		$(wpa_psk_key).text($(twkey + "_psk_pass_phrase").val());
	}
}


function onPageLoad(){
	if ($("#wlan0_enable").val() === "1")
		show_info("wlan0", "host", "24G");
	else
		$("#wlan0").hide();

	if ($("#wlan1_enable").val() === "1")
		show_info("wlan1", "host", "5G");
	else
		$("#wlan1").hide();

	if ($("#wlan0_vap1_enable").val() === "1")
	//	show_info("wlan0_vap1", "guest", "24G");
		$("#wlan0_guest").hide();
	else
		$("#wlan0_guest").hide();

	if ($("#wlan1_vap1_enable").val() === "1")
		//show_info("wlan1_vap1", "guest", "5G");
		$("#wlan1_guest").hide();
	else
		$("#wlan1_guest").hide();
}

onPageLoad();
</script>
