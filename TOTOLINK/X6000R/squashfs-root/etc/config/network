
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fd64:c668:2266::/48'

config interface 'lan'
	option device 'br-lan'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option macaddr '1C:F4:08:53:15:6C'
	option secondipaddr '10.188.188.188'
	option secondnetmask '255.255.255.255'
	option ipv6 '0'
	option lan_dns_server '0'

config interface 'wan'
	option device 'eth1'
	option proto 'dhcp'
	option hostname 'TOTOLINK'
	option macaddr '1C:F4:08:53:15:6D'
	option defmacaddr '1C:F4:08:53:15:6D'
	option peerdns '1'
	option dmtu '1500'
	option smtu '1500'
	option pmtu '1492'
	option mtu '1500'
	option usernames ''
	option password ''
	option service ''
	option lcp_echo_enable '1'
	option ttl_way '1'
	option keepalive '5,30'
	option ipv6 '0'
	option ppp_spectype '0'
	option ppp_mode '0'



config interface 'wan6'
	option ifname '@wan'
	option proto 'dhcpv6'
	option disabled '1'

config interface 'vpn'
	option l2tp_mode '0'
	option pptp_mode '0'
	option mtu '1460'
	option mppc '1'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_vlan
	option device 'switch0'
	option vlan '1'
	option ports '0 1 2 3 4 6'

config sys 'iptv'
	option mrEnable '1'
	option mrQleave '3'
	option etherIgmp '1'
	option iptvEnabled '0'
	option iptvVer '5'
	option vlanEnabled '0'
	option wanStrategy '0'
	option vlanVidCpu '10'
	option vlanPriCpu '0'
	option vlanVidLan1 ''
	option vlanPriLan1 '0'
	option vlanVidLan2 ''
	option vlanPriLan2 '0'
	option vlanVidLan3 ''
	option vlanPriLan3 '0'
	option vlanVidLan4 ''
	option vlanPriLan4 '0'

config device
        option type 'bridge'
        option name 'br-lan'
        list ports 'eth0'
        option bridge_empty '1'
	option macaddr '1C:F4:08:53:15:6C'

