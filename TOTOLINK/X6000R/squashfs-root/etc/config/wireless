
config wifi-device 'radio_2g'
	option disabled '0'
	option country 'CN'
	option txpower_lvl '2'
	option channel '1'
	option band '2.4G'
	option hw_show '16'
	option ht_show '2'
	option max_sta_num '64'
	option roam_threshold '0'
	option twt '1'
	option mumimo '1'
	option ofdma '1'
	option acl_mode '0'
	option acl_num '0'
	option acl_maclist ''
	option wifiVlanID '0'

config wifi-iface 'main_2g'
	option device 'radio0'
	option ifname 'ra0'
	option ssid 'WiFi-2.4G'
	option authmode 'OPEN'
	option encryption 'NONE'
	option key ''
	option hidden '0'
	option isolate '0'

config wifi-iface 'mesh_2g'
	option device 'radio0'
	option ifname 'ra1'
	option ssid 'WiFi-MESH-2G'
	option authmode 'OPEN'
	option encryption 'NONE'
	option key ''
	option hidden '0'
	option disabled '1'
	option allow_access '0'
	option mapmode '0'
	option devicerole '0'

